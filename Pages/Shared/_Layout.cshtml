    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <header class="sticky-top">
        <nav class="navbar navbar-expand-xl navbar-toggleable-xl navbar-dark px-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-page="/Index"><img src="/img/logo-company-name-horizontal-dark.png" alt="Bootstrap" class="navbar-logo" /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-center fs-5" id="navbarNavDropdown">
                    <ul class="navbar-nav pt-5 pt-xl-0">
                        <li class="nav-item">
                            <a class="nav-link" href="/about-us">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/services-overview">Our Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/contact-us">Contact Us</a>
                        </li>
                        <li class="nav-item d-xl-none">
                            <a href="/contact#ContactSection">
                                <button class="w-100 py-3 my-5" style="max-width: 500px;">Schedule a Call</button>
                            </a>
                        </li>
                    </ul>
                </div>
                <a href="/contact#ContactSection" class="d-none d-xl-block">
                    <button class="fs-6 px-3 py-2">Schedule a Call</button>
                </a>
            </div>
        </nav>
    </header>
    <div class="container-fluid px-0">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
        <div class="footer-top">
            <div class="container row justify-content-between mx-auto">
                <div class="col-auto mb-3 order-lg-2">
                    <h4 class="pb-3">Company Info</h4>
                    <ul class="list-unstyled">
                        <li>949 South Coast Drive, Suite 280</li>
                        <li>Costa Mesa, CA 92626</li>
                        <li><a href="tel:9495206805">(949) 520-6805</a></li>
                        <br />
                        <li>Mon-Fri: 8am - 6pm (PST)</li>
                        <li>Sat-Sun: Closed</li>
                    </ul>
                </div>
                <div class="col-auto mb-3 order-lg-3">
                    <h4 class="pb-3">Quick Links</h4>
                    <ul class="list-unstyled">
                        <!-- <li><a href="/faq" class="">FAQ</a></li> -->
                        <li><a href="/index" class="">Home</a></li>
                        <li><a href="/services-overview" class="">Services</a></li>
                        <li><a href="/about-us" class="">About Us</a></li>
                        <li><a href="/contact-us" class="">Contact</a></li>
                        <li><a href="/privacy-policy" class="">Privacy Policy</a></li>
                        <li><a href="/terms-of-service" class="">Terms of Service</a></li>
                    </ul>
                </div>
                <div class="col-auto mx-auto mx-xl-0 order-lg-1">
                    <div class="mb-4"><img src="/img/logo-company-name-horizontal-dark.png" alt="Simple Cyber Tech Logo" class="footer-logo img-fluid"/></div>
                    <ul class="list-inline d-flex justify-content-between">
                        <li class="list-inline-item">
                            <a href="https://www.linkedin.com/company/simplecybertech/" target="_blank" class="text-decoration-none d-flex align-items-center">
                                <img src="/img/social-linkedin-square.svg" alt="LinkedIn Icon" width="35" height="35" class="footer-social-icon me-2">
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#" class="text-decoration-none d-flex align-items-center">
                                <img src="/img/social-twitter-square.svg" alt="Twitter/X Icon" width="35" height="35" class="footer-social-icon me-2" />
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#" class="text-decoration-none d-flex align-items-center">
                                <img src="/img/social-facebook-square.svg" alt="Facebook Icon" width="35" height="35" class="footer-social-icon me-2" />
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#" class="text-decoration-none d-flex align-items-center">
                                <img src="/img/social-instagram-square.svg" alt="Instagram Icon" width="35" height="35" class="footer-social-icon me-2" />
                            </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="#" class="text-decoration-none d-flex align-items-center">
                                <img src="/img/social-youtube-square.svg" alt="YouTube Icon" width="35" height="35" class="footer-social-icon me-2" />
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="container text-center py-3">
            &copy; 2026 Simple Cyber Technology.  All rights reserved.</span>
